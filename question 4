total = 0

# Fixing the loop conditions and calculation
for i in range(5):
    for j in range(3):
        if i + j == 5:
            total += i + j
        else:
            total += i - j  # Fixing the assignment error

counter = 0

# Fixing the while loop conditions
while counter < 5:
    if total < 13:
        total += 1
    elif total > 13:
        total = 1
    else:
        counter += 2
